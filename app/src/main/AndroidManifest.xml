<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivities.addMoneyActivity"></activity>
        <activity android:name=".MainActivities.CreateNewSessionStart" />
        <activity android:name=".MainActivities.CreateNewSession_3" />
        <activity android:name=".MainActivities.CreateNewSession_2" />
        <activity android:name=".MainActivities.AddPaymentMethodActivity" />

        <service
            android:name=".MainActivities.notifications"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".MainActivities.CreateNewSession" />
        <activity
            android:name=".MainActivities.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivities.MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>