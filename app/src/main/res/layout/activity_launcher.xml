<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame_Layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#006FB9"
    tools:context=".SourceCode.activities.LauncherActivity">

    <TextView
        android:id="@+id/tTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="304dp"
        android:layout_marginTop="392dp"
        android:fontFamily="@font/rounded_bold"
        android:text="T"
        android:textColor="#FFFFFF"
        android:textSize="90sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/uTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="240dp"
        android:layout_marginTop="392dp"
        android:fontFamily="@font/rounded_bold"
        android:text="U"
        android:textColor="#FFFFFF"
        android:textSize="90sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="392dp"
        android:fontFamily="@font/rounded_bold"
        android:text="C"
        android:textColor="#FFFFFF"
        android:textSize="90sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lTextView"
        android:layout_width="wrap_content"
